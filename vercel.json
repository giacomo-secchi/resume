{
    "version": 2,
    "installCommand": "npm install",
    "outputDirectory": "dist",
    "buildCommand": "npm run build",
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node",
            "config": {
                "command": "npm run build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}

